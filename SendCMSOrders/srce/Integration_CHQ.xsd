<xs:schema targetNamespace="http://fncinc.com/CHQ" elementFormDefault="qualified" xmlns="http://fncinc.com/CHQ" xmlns:mstns="http://fncinc.com/CHQ" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:in="http://fncinc.com/Integration.xsd" attributeFormDefault="unqualified">
  <xs:import namespace="http://fncinc.com/Integration.xsd" schemaLocation="Integration.xsd"/>
  <xs:complexType name="CreateOrderRqType">
    <xs:sequence>
      <xs:element name="GroupId" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A Unique Identifier (PK) for the Group (within the CHQ Client’s organization) that is associated with or responsible for this order. The full list of PKs for all client groups will be provided to the client by FNC.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcPrsnPk" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A Unique Identifier (PK) for the Processor that is associated with or responsible for this order. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcComments" type="in:string1024" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation> Processor Comments </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrderInfo" type="OrderInfoType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Includes information related to the order, such as Loan Number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PropertyAddress" type="AddressInfoType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Includes information related to the Property Address, such as Street # and Prefix</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PropertyTypeId" type="in:string10" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Type of property (Examples: Condo Hotel, Condominium, SFR Attached)</xs:documentation>
          <xs:documentation>Enumeration details contained in Editable Dropdown UI</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LoanPurposeId" type="LoanPurposeOption" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Purpose for the Loan (Examples: Purchase, Refinance)</xs:documentation>
          <xs:documentation>Enumeration details contained in LoanPurposeOption</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HpmlOrder" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>whether the order is high Priced mortagage loan (True/False)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OccupancyTypeId" type="OccupancyTypeOption" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Property’s occupancy type (Examples: Investor, Primary Residence)</xs:documentation>
          <xs:documentation>Enumeration details contained in OccupancyTypeOption</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerEstimate" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Owner’s Estimate of value of the property</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SalesPrice" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Sales Price for the property</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OriginalPurchasePrice" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Original Purchase Price of the Property</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LoanAmount" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Requested Loan Amount</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OutstandingLien" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Total Amount of Lien on Property</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CustomerPrice" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Fee CHQ client is charging its customer to fulfill the loan request (if applicable)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProviderFee" type="xs:decimal" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Fee CHQ client will pay the Service Provider to fulfill service requested</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceId" type="ServiceOption" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Enumeration details contained in ServiceOption</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceProviderID" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Service Provider ID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BorrowerContact" type="ContactType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Includes Information related to the borrower, such as First Name and Last Name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AlternateContact" type="AltContactType" minOccurs="0" maxOccurs="20">
        <xs:annotation>
          <xs:documentation>Includes Information related to an Alternate Contact, such as First Name and Last Name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MCRequired" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Used to denote that the 1004MC addendum is required for this order.  (Pass a value of 1 if it is an Rush order. Otherwise, pass nothing or a 0.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsBorrowerCC" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Denotes where the borrower's Credit Card is billed previously for the order (1 if it is charged. Otherwise, pass nothing or a 0.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentParty" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Used to trigger credit card collections email and to set the status of the order to "WAITING FOR PAYMENT"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ParentDocID" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Used to assign child services</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ByPassDupChk" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Bypass duplicate check flag - True/False 1/0</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UsePreviousServiceProvider" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Use Previous Service Provider - True/False 1/0</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UsePreviousReviewer" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Use Previous Reviewer - True/False 1/0</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UsePreviousOrderAttributes" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Use Previous Order Attributes - True/False 1/0</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CustomerID" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>CustomerID to be associated with the order.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BatchName" type="in:string50" minOccurs="0" maxOccurs="1" default="">
        <xs:annotation>
          <xs:documentation>Optional field for Batch association</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="CreateOrderRs">
    <xs:annotation>
      <xs:documentation>This is a synchronous call.  The response is expected to return a FolderID and DocID for later reference.  The response may also return a processing error and message. The Client System should store the Folder ID and Doc Id in its system.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:ResponseHdrType"/>
        <xs:element name="Tracking" type="OrderTrackingType" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="OneViewReviewRqType">
    <xs:sequence>
      <xs:element name="LoanNumber" type="in:string60" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Loan Number for One View Review</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReconciledValue" type="xs:int" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Reconciled Value</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReviewDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Review Date</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Comments" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Comment</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CommentType" type="in:string50" fixed="Servicer">
        <xs:annotation>
          <xs:documentation>Comment Type</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="CreateOneViewReviewRs">
    <xs:annotation>
      <xs:documentation>The response is expected to return a LoanRevSummaryId, FolderID and DocID for later reference.  The response may also return a processing error and message. The Client System should store the Folder ID and Doc Id in its system.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:ResponseHdrType"/>
        <xs:element name="Tracking" type="OneViewTrackingType" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="OneViewTrackingType">
    <xs:annotation>
      <xs:documentation>Order details to provide tracking between LOS and CHQ</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LoanRevSummaryId" type="xs:int" minOccurs="1">
        <xs:annotation>
          <xs:documentation>A One View Review ID used in CMS.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FolderId" type="in:string20" minOccurs="1">
        <xs:annotation>
          <xs:documentation>A tracking number used in CMS.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocId" type="in:string20" minOccurs="1">
        <xs:annotation>
          <xs:documentation>A tracking number used in CMS. DocId is also used for later status updates. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LoanNumber" type="in:string60" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Loan Number; (Echoing back the received Loan Number)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="UploadFileRq">
    <xs:annotation>
      <xs:documentation>A file uplaod request to CMS</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType"/>
        <xs:element name="Tracking" type="UploadFileType" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="UploadFileType">
    <xs:annotation>
      <xs:documentation>Provide information of the file to be uploaded to FNC</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DocId" type="in:string20" minOccurs="1">
        <xs:annotation>
          <xs:documentation>A tracking number used in CMS. DocId is also used for later status updates. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FileTypeId" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Enumeration Description
            AP4REV		  Appraisal for Review
            APINV		    Invoice
            APSL		    Appraisal Report
            BPO		      BPO Report
            COMPLT		  Compliance Letter
            IPSL		    Inspection Report
            MISMO       MISMO XML
            OTHER		    Other Documents
            PRCHAGRMT	  Purchase Agreement
            QCV_APSL	  QC Vigilance Report
            REVPDF		  Review
            AVM         AVM
            CUSTINV     Customer Invoice
            DEED        Deed
            LEGAL       Legal
            ORDERSHEET  Order Sheet
            PLANSSPECS  Plans and Specs
            PAPSL       Previous Appraisal
            QUESTIONS   Questionnaires
            RECONCILE   Reconciliation
            SEQUESTER   Sequestered - Resubmission
            SURVEY      Surveys
            TPSL        Title Document
            VALIDCHK    Validity Check
            HOACERT     HOA Certification
            AERIAL      Aerial Photo
            AUTHLETTER  Authorization Letter
            COSTEST     Cost Breakdown or Estimate
            FLOODMAP    Flood Map
            GOVREC      Government Records
            INEXSTATE   Income/Expense Statements
            INSPCHECK   Inspection Checklist
            INDEPDOC    Internal Department Document
            RENTROLL    Leases/Rent Roll
            BROCHRE     Listing/Marketing Brochure
            OTHERBID    Other Bid Info
            OTHERENVIR  Other Environmental Report
            PHOTO       Photo
            SKETCH      Property Sketch
            SALEDATA    Sales Data
            SITEPLAN    Site Plan
            SUPPLEMENT  Supplement
            TAX         Tax Files
            APPEAL      Appeal
            EVALUATION  Evaluation
            APSLREST    Restricted Appraisal
            WORKFILE    Work File
            WI          Wiring Instructions
            CPL         Closing Protection Letter
            REVS        Review Summary
            VINV        Vendor Invoice
            CNSTRPT     Construction Report
            SUPDOC      Supporting Documents
            PROPFIN     Property Financials
            PRIRPTS     Prior Reports
            ADDINFO     Additional Information
            ENVQUE      Environmental Questionnaire
            ALTDISC     Alternative Disclosure Form
            CNSRPTCD    Construction Report Card
            LOADINV     Loan Admin Invoice
            REVRPT2     Review Report
            ENVRPT      Environmental Report
            AREAMAP     Area/Street Map
            COMPDATA    Supporting Comp Data
            DATAREP     Data Verify Report
            DISDOC      Disaster Document
            DISP        Dispute Form
            EVAL        Desktop Valuation Report
            FIELDREV    Field Review
            OADI_INV    Appraisal Invoice
            QCDATA      QC Additional Data
            RBELL       Red Bell Appraisal
            RCPT        Receipt
            RQREPORT    Real Quest Report
            TITLECOM    Title Commitment
            UNDER       Underwriting Checklist
            WATCHLIST   Watch List
          </xs:documentation>
        </xs:annotation>        
      </xs:element>
      <xs:element name="FileName" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>A treference name about this file. It is not used in CMS, but for the sender to track.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="OrderTrackingType">
    <xs:annotation>
      <xs:documentation>Order details to provide tracking between LOS and CHQ</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FolderId" type="in:string20" minOccurs="1">
        <xs:annotation>
          <xs:documentation>A tracking number used in CMS.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DocId" type="in:string20" minOccurs="1">
        <xs:annotation>
          <xs:documentation>A tracking number used in CMS. DocId is also used for later status updates. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LoanNumber" type="in:string60" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Loan Number; (Echoing back the received Loan Number)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="StatusUpdateType">
    <xs:sequence>
      <xs:element name="DocId" type="in:string20" minOccurs="1"/>
      <xs:element name="LoanNumber" type="in:string60" minOccurs="1"/>
      <xs:element name="StatusId" type="in:string10" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            StatusID	Description
            M	Accepted
            AC	Provider Accepts w/ Condition
            D	Draft Received from Provider
            C	Cancelled – General
            CD	Cancelled – Duplicate Request
            CR	Cancelled – By Request
            CE	Cancelled – Refund Due
            LA	Approved – As Is
            LC	Approved – With Conditions
            LD	Declined
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description" type="in:string30" minOccurs="1"/>
      <xs:element name="ReportUrl" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>URL linking to order related documents, provided when the order is closed (L status)</xs:documentation>
        </xs:annotation>
      </xs:element>


      <xs:element name="UCDPReadyFileURL" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Link to generate UCDP Ready File</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="FannieSSR" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Flow Based Fannie and Freddie SSR </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FreddieSSR" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Flow Based Fannie and Freddie SSR</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FHASSR" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Flow Based FHA SSR</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InvoiceUrl" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Link to order invoice if present</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PropertyScan" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Link to Property Scan</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarketReport" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Link to Market Report</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValueSight" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Link to Value Sight</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="UCDPElements" type="UCDPInfo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Returns the UCDP Info displayed on the ucdp tab</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EADElements" type="EADInfo" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Returns the EAD Info displayed on the EAD tab</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsPreDelivery" type="xs:boolean" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Returns whether the order was a predelivery or not</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AssociatedFiles" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
