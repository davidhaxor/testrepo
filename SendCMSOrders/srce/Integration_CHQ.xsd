        <xs:element name="Header" type="in:ResponseHdrType"/>
        <xs:element name="Tracking" type="OrderTrackingType" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="DataFeedback" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <!--  use this to request status, CHQ will seed back status update 
      we will use doc-id as our preference mathod to locate order
-->
  <xs:element name="RetrieveOrderStatusRq">
    <xs:annotation>
      <xs:documentation>The Status Updates Touch Point originates from the LOS. This update is triggered when the order’s status status request is received by CHQ.  Order updates are transferred from CHQ to the Client System (ordering system).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType">
          <xs:annotation>
            <xs:documentation>
              SourceApp = LOS
              TargetApp = CHQ
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Tracking" type="OrderTrackingType" minOccurs="1" maxOccurs="1"/>
        <xs:element name="IsDeliverFinal" type="xs:boolean" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- When an order was created in CHQ, we will post this back to LOS. We expect a standard Ack message -->
  <xs:element name="ReverseCreateOrderRq">
    <xs:annotation>
      <xs:documentation>
        The Ordering Process originates with the creation of an order placed in CHQ.  The order is transferred from CHQ and initiates a request to “Create Order” in the LOS.
      </xs:documentation>
      <xs:documentation>Note:  If orders will not be initiated within CHQ this section is not a requirement.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType">
          <xs:annotation>
            <xs:documentation>
              SourceApp = CHQ
              TargetApp = LOS
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Order" type="CreateOrderRqType"/>
        <xs:element name="Tracking" type="OrderTrackingType" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  returning xml for RetrieveOrderStatusRq -->
  <xs:element name="RetrieveOrderStatusRs">
    <xs:annotation>
      <xs:documentation>Response containing order status information</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:ResponseHdrType"/>
        <xs:element name="StatusUpdate" type="StatusUpdateType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
