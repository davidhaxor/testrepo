      </xs:sequence>
    </xs:complexType>
  </xs:element>



  <xs:element name="ChargeInformationRs">
    <xs:annotation>
      <xs:documentation>Response for the ChargeInformationRq will contain the lineitem information for each charge</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:ResponseHdrType"/>
        <xs:element name="DocId" type="in:string20" minOccurs="1"></xs:element>
        <xs:element ref="ChargeLineItems" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="SendChargesRq">
    <xs:annotation>
      <xs:documentation>The SendChargesRq will contain the lineitem information for each charge for a doc</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType"/>
        <xs:element name="DocId" type="in:string20" minOccurs="1"></xs:element>
        <xs:element ref="ChargeLineItems" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="SendDatesRq">
    <xs:annotation>
      <xs:documentation>The SendDatesRq will contain the Date information for a doc</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType"/>
        <xs:element name="DocId" type="in:string20" minOccurs="1"></xs:element>
        <xs:element name="AssignedDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the order was assigned to the current Service Provider</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DueFromVendorDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the appraisal is due from the Service Provider</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DueToCustDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the appraisal and decision is due to the Customer</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ConfirmationDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the Service Provider accepted the order</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="InspectionDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the Service Provider is scheduled to perform the inspection of the Property</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DraftDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the draft was received from the Service Provider</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="CancelDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the order was canceled</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="AcceptDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the Service Provider accepted the order or accepted the order with conditions</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="CompletionDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the order was completed (Logged Out/Post or Billed)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="OrderDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the order was created/placed in CMS       </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ReviewerAssignDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date a reviewer was assigned to the order</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ProcessorAssignDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date a processor was assigned to the order</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="LoanOfficerAssignDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date a Loan Officer/Broker/Correspondent was assigned to the order</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="FirstCompletionDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the order was first completed (Logged Out/Post or Billed)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="FirstDraftDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The date the first draft was received from the Service Provider</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ClosingDt" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>The expected closing date of the loan</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="InvoiceStatusRs">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Invoice"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="InvoiceData">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="InvoiceUrl" type="xs:string" minOccurs="0"/>
        <xs:element name="AsOf" type="xs:dateTime" minOccurs="1"/>
        <xs:element name="CustomerPk" type="xs:int">
          <xs:annotation>
            <xs:documentation>A Unique Identifier (PK) for the Customer associated with this order. The list of PKs for all customers will be provided to the client by FNC.</xs:documentation>
            <xs:documentation>If no customer is assigned to the order 0 will be passed</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Customer" type="OrganizationType" />
        <xs:element name="ServiceProviderCompany" type="OrganizationType" />
        <xs:element name="ServiceProvider" type="PersonType" />

        <xs:element name="BorrowerName" type="xs:string"/>

        <xs:element ref="Invoice"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Invoice">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="LineItems" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ChargeLineItems">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ChargeLineItem" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Charges">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Charge" minOccurs="0" maxOccurs="20"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Charge">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ChargeNumber" type="in:GUID" minOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="LineItems">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="LineItem" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="LineItem">


    <xs:annotation>
      <xs:documentation>

      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AdjustmentReason" type="AdjReason" minOccurs="0" />
        <xs:element name="CustomerPrice" type="xs:decimal" minOccurs="0" />
        <xs:element name="CustomerChargeStatus" type="ChargeStatus" minOccurs="0" />
        <xs:element name="CustomerInvoiceDate" type="xs:dateTime" minOccurs="0" />
        <xs:element name="VendorFee" type="xs:decimal" minOccurs="0" />
        <xs:element name="VendorChargeStatus" type="ChargeStatus" minOccurs="0" />
        <xs:element name="VendorInvoiceDate" type="xs:dateTime" minOccurs="0" />

      </xs:sequence>
      <xs:attribute name="ChargeNo" type="in:GUID" use="required"/>
    </xs:complexType>
  </xs:element>



  <xs:element name="ChargeLineItem">
    <xs:annotation>
      <xs:documentation>

      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="CustomerAdjustmentReason" type="xs:string" minOccurs="0" />
        <xs:element name="CustomerPrice" type="xs:decimal" minOccurs="0" />
        <xs:element name="CustomerChargeStatus" type="ChargeStatus" minOccurs="0" />
        <xs:element name="CustomerPaidDate" type="xs:dateTime" minOccurs="0" />
        <xs:element name="CustomerInvoiceDate" type="xs:dateTime" minOccurs="0" />
        <xs:element name="VendorFee" type="xs:decimal" minOccurs="0" />
        <xs:element name="VendorChargeStatus" type="ChargeStatus" minOccurs="0" />
        <xs:element name="VendorAdjustmentReason" type="xs:string" minOccurs="0" />
        <xs:element name="VendorPaidDate" type="xs:dateTime" minOccurs="0" />
        <xs:element name="VendorInvoiceDate" type="xs:dateTime" minOccurs="0" />
        <xs:element name="InvoiceNumber" type="in:string20" minOccurs="0"/>
        <xs:element  name="ChargeNo" type="in:GUID" minOccurs="0"/>
      </xs:sequence>

    </xs:complexType>
  </xs:element>
  <xs:simpleType name="AdjReason">
    <xs:annotation>
      <xs:documentation>
        Simple type for a ajustment reason (enum)
        A                 Addendum
        AC                Atypical Cancellation
        DF                Distance Fee
        ED                Employee Discount
        EF                Exception Fee
        EP                Exception Price
        MC                Management Concession
        NA                New Assignment/Transfer
        NewA              New Assignment
        OTH               Other
        RA                Reassignment
        RUSH              Rush Fee
        TF                Trip Fee/Research Fee
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="A"/>
      <xs:enumeration value="AC"/>
      <xs:enumeration value="DF"/>
      <xs:enumeration value="ED"/>
      <xs:enumeration value="EF"/>
      <xs:enumeration value="EP"/>
      <xs:enumeration value="MC"/>
      <xs:enumeration value="NA"/>
      <xs:enumeration value="NewA"/>
      <xs:enumeration value="OTH"/>
      <xs:enumeration value="RA"/>
      <xs:enumeration value="RUSH"/>
      <xs:enumeration value="TF"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ChargeStatus">
    <xs:annotation>
      <xs:documentation>
        Simple type for a charge stuatus (enum)
        I = invoiced
        P = Pending
        PD = Paid
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="NA"/>
      <xs:enumeration value="I"/>
      <xs:enumeration value="P"/>
      <xs:enumeration value="PD"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="OrganizationType">
    <xs:sequence>
      <xs:element name="CompanyID" type="xs:string"/>
      <xs:element name="CompanyName" type="xs:string"/>
      <xs:element name="CompanyAddress" type="AddressInfoType" minOccurs="0" />
      <xs:element name="Phone" type="xs:string" />
      <xs:element name="AltPhone" type="xs:string" minOccurs="0" />
      <xs:element name="Email" type="xs:string" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="PersonType">
    <xs:sequence>
      <xs:element name="PersonID" type="xs:string"/>
      <xs:element name="Name" type="xs:string"/>
      <xs:element name="Address" type="AddressInfoType" minOccurs="0" />
      <xs:element name="Phone" type="xs:string" />
      <xs:element name="AltPhone" type="xs:string" minOccurs="0" />
      <xs:element name="Email" type="xs:string" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="UCDPInfo">
    <xs:sequence>
      <xs:element name="FannieStatus"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="FreddieStatus"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="UCDPID"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="LoanNumber"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="SubmissionID"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name ="RecentSubmission"  minOccurs="0" type="xs:dateTime" ></xs:element>
      <xs:element name="RecentFinding"   minOccurs="0" type="xs:dateTime" ></xs:element>
      <xs:element name="GateWayStatus"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="GatewayNote" minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="CUScore" minOccurs="0" type="xs:float" ></xs:element>

    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="EADInfo">
    <xs:sequence>
      <xs:element name="FHAStatus"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="EADID"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="LoanNumber"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="SubmissionID"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="RecentSubmission"  minOccurs="0" type="xs:dateTime" ></xs:element>
      <xs:element name="RecentFinding"   minOccurs="0" type="xs:dateTime" ></xs:element>
      <xs:element name="GateWayStatus"  minOccurs="0" type="xs:string" ></xs:element>
      <xs:element name="GatewayNote" minOccurs="0" type="xs:string" ></xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:element name="UpdateOrderStatusRq">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType">
          <xs:annotation>
            <xs:documentation>
              SourceApp = LOS
              TargetApp = CHQ
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="OrderStatusUpdate" type="UpdateOrderStatusRqType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="UpdateOrderStatusRqType">
    <xs:sequence>
      <xs:element name="DocId" type="in:string20" minOccurs="1">
        <xs:annotation>
          <xs:documentation>DocId of order status to change.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StatusId" type="in:string10" minOccurs="1">
        <xs:annotation>
          <xs:documentation>
            StatusID	Description
            M	Accepted
            AC	Provider Accepts w/ Condition
            D	Draft Received from Provider
            C	Cancelled – General
            CD	Cancelled – Duplicate Request
            CR	Cancelled – By Request
            CE	Cancelled – Refund Due
            LA	Approved – As Is
            LC	Approved – With Conditions
            LD	Declined
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  
  <xs:element name="FeeInformationRq">
    <xs:complexType>
      <xs:annotation>
        <xs:documentation>API access to Good Faith Appraisal Fee Estimator. Response to this post will be of type</xs:documentation>
      </xs:annotation>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType">
          <xs:annotation>
            <xs:documentation>
              SourceApp = LOS
              TargetApp = CHQ
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="LoanData" type="FeeInformationRqType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FeeInformationRs">
    <xs:complexType>
      <xs:annotation>
        <xs:documentation>Immediate response to FeeInformationRq, containing a GUID tracking Id. As fee determination can take significant time, the actual fee will be returned in a follow-up post of FeeInformationResultRq containing the given tracking Id.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
        <xs:element name="Header" type="in:ResponseHdrType"></xs:element>
        <xs:element name="FeeInformationTrackingId" type="in:GUID" minOccurs="1"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FeeInformationResultRq">
    <xs:complexType>
      <xs:annotation>
        <xs:documentation>Follow-up post containing the given tracking Id as a reference and the Good Faith Appraisal Fee Estimation.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
        <xs:element name="Header" type="in:RequestHdrType"></xs:element>
        <xs:element name="QuoteData" type="FeeInformationResultRqType" minOccurs="1"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="FeeInformationRqType">
    <xs:sequence>
      <xs:element name="PropertyAddress" type="in:string70" minOccurs="1"></xs:element>
      <xs:element name="City" type="in:string30" minOccurs="1"></xs:element>
      <xs:element name="State" type="in:State" minOccurs="1"></xs:element>
      <xs:element name="Zip" type="in:ZipPlus4" minOccurs="1"></xs:element>
      <xs:element name="County" type="in:string30" minOccurs="1"></xs:element>
      <xs:element name="ServiceType" type="ServiceOption" minOccurs="1"></xs:element>
      <xs:element name="SubLoanType" type="SubLoanTypeOption" minOccurs="0"></xs:element>
      <xs:element name="SalesPrice" type="xs:decimal" minOccurs="0"></xs:element>
      <xs:element name="OwnerEstimatedValue" type="xs:decimal" minOccurs="0"></xs:element>
      <xs:element name="CustomerOrgPk" type="xs:int" minOccurs="0"></xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FeeInformationResultRqType">
    <xs:sequence>
      <xs:element name="FeeInformationTrackingId" type="in:GUID" minOccurs="1"></xs:element>
      <xs:element name="PropertyAddress" type="in:string70" minOccurs="1"></xs:element>
      <xs:element name="City" type="in:string30" minOccurs="1"></xs:element>
      <xs:element name="State" type="in:State" minOccurs="1"></xs:element>
      <xs:element name="Zip" type="in:ZipPlus4" minOccurs="1"></xs:element>
      <xs:element name="County" type="in:string30" minOccurs="1"></xs:element>
      <xs:element name="ServiceType" type="ServiceOption" minOccurs="1"></xs:element>
      <xs:element name="SubLoanType" type="SubLoanTypeOption" minOccurs="0"></xs:element>
      <xs:element name="SalesPrice" type="xs:decimal" minOccurs="0"></xs:element>
      <xs:element name="OwnerEstimatedValue" type="xs:decimal" minOccurs="0"></xs:element>
      <xs:element name="CustomerOrgPk" type="xs:int" minOccurs="0"></xs:element>
      <xs:element name="AppraisalFeeFound" type="xs:boolean"></xs:element>
      <xs:element name="EstimatedAppraisalFee" type="xs:decimal"></xs:element>
    </xs:sequence>
  </xs:complexType>

</xs:schema>
